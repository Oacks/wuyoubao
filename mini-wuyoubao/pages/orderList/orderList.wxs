var getStatus = function(val) {
    var num = parseInt(val);
    console.log(num);
    switch(num) {
        case 1:
        return '申请'
        case 2:
        return '审批盖章'
        case 3:
        return '合同审核'
        case 4:
        return '发卡'
        case 5:
        return '生效'
    }
}

module.exports = {
    getStatus: getStatus
}